{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "outDir": "./dist/skalarly-frontend/server",
    "target": "ES2022",
    "moduleResolution": "node",
    "module": "ES2022",
    "types": ["node"],
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true
  },
  "files": [
    "src/main.server.ts"
  ],
  "exclude": ["node_modules"]
}

