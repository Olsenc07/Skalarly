{
  "name": "skalarly-frontend",
  "version": "0.0.0",
  "description": "Front-end for Skalarly application",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --source-map",
    "build": "ng lint && ng build",
    "build:stats": "ng build --configuration=production --stats-json --source-map",
    "analyze": "webpack-bundle-analyzer dist/skalarly-frontend/stats.json",
    "watch": "ng build --watch --configuration=development --source-map",
    "test": "ng test",
    "lint": "ng lint",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "build:dev": "ng lint && ng build --configuration=development --stats-json",
    "build:prod": "ng lint && ng build --configuration=production",
    "start:prod": "node dist/server/main.js",
    "e2e": "start-server-and-test build-and-deploy:prod http://localhost:4200 cypress:run",
    "build:ssr": "ng build --configuration=production && ng run skalarly-fs:server:production --stats-json",
    "analyze:ssr": "webpack-bundle-analyzer dist/server/stats.json",
    "test:ssr": "node dist/server/main.js"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^17.0.8",
    "@angular/common": "^17.0.8",
    "@angular/compiler": "^17.0.8",
    "@angular/core": "^17.0.8",
    "@angular/forms": "^17.0.8",
    "@angular/material": "^17.0.4",
    "@angular/platform-browser": "^17.0.8",
    "@angular/platform-browser-dynamic": "^17.0.8",
    "@angular/platform-server": "^17.0.8",
    "@angular/router": "^17.0.8",
    "@angular/ssr": "^17.0.8",
    "@babel/cli": "^7.23.4",
    "@babel/core": "^7.23.7",
    "@babel/preset-env": "^7.23.8",
    "express": "^4.18.2",
    "ngx-skeleton-loader": "^8.1.0",
    "node": "^20.10.0",
    "rxjs": "^7.4.0",
    "tslib": "^2.6.2",
    "uuid": "^9.0.1",
    "webpack-bundle-analyzer": "^4.10.1",
    "zone.js": "^0.14.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.0.8",
    "@angular-eslint/builder": "^17.1.1",
    "@angular-eslint/eslint-plugin": "^17.1.1",
    "@angular-eslint/eslint-plugin-template": "^17.1.1",
    "@angular-eslint/schematics": "^17.1.1",
    "@angular-eslint/template-parser": "^17.1.1",
    "@angular/cli": "^17.0.8",
    "@angular/compiler-cli": "^17.0.8",
    "@types/express": "^4.17.17",
    "@types/jasmine": "^5.1.4",
    "@types/node": "^18.18.0",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.11.0",
    "cypress": "^13.6.1",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jasmine": "^4.1.3",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-rxjs-angular": "^2.0.1",
    "jasmine-core": "^5.1.1",
    "karma": "^6.4.2",
    "karma-chrome-launcher": "^3.2.0",
    "karma-coverage": "^2.2.1",
    "karma-firefox-launcher": "^2.1.2",
    "karma-jasmine": "^5.1.0",
    "karma-jasmine-html-reporter": "^2.1.0",
    "karma-safari-launcher": "^1.0.0",
    "tough-cookie": "4.1.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.2.2",
    "webpack": "^5.89.0"
  },
  "overrides": {
    "axios": "^1.6.2"
  }
}
