{
  "name": "skalarly-frontend",
  "version": "0.0.0",
  "description": "Frontend for Skalarly application",
  "type": "module",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "npm run build:server && npm run build:client",
    "build:client": "ng build --configuration=production",
    "build:server": "ng run skalarly-frontend:server:production",
    "build:client:dev": "ng build --configuration=development",
    "build:server:dev": "ng run skalarly-frontend:server:development && ng build --configuration=development",
    "serve:ssr": "node ../dist/skalarly-frontend/server/main.js",
    "serve:ssr:dev": "npm run build:client:dev && npm run build:server:dev && node ../dist/skalarly-frontend/server/main.js",
    "deploy:ssr": "npm run build && npm run serve:ssr",
    "lint": "ng lint",
    "test": "ng test",
    "e2e": "start-server-and-test serve http://localhost:4200 cypress:run",
    "analyze": "ng build --prod --stats-json && webpack-bundle-analyzer ../dist/skalarly-frontend/",
    "predeploy": "npm run build"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^17.2.1",
    "@angular/cli": "^17.2.0",
    "@angular/common": "^17.2.1",
    "@angular/core": "^17.2.1",
    "@angular/forms": "^17.2.1",
    "@angular/material": "^17.2.0",
    "@angular/platform-browser": "^17.2.1",
    "@angular/platform-server": "^17.2.1",
    "@angular/router": "^17.2.1",
    "ngx-image-cropper": "^7.2.1",
    "ngx-skeleton-loader": "^8.1.0",
    "node": "^20.10.0",
    "rxjs": "^7.4.0",
    "tslib": "^2.6.2",
    "uuid": "^9.0.1",
    "zone.js": "^0.14.2"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "^17.0.1",
    "@angular-devkit/build-angular": "^17.1.3",
    "@angular-eslint/builder": "^17.2.1",
    "@angular-eslint/eslint-plugin": "^17.2.1",
    "@angular-eslint/eslint-plugin-template": "^17.2.1",
    "@angular-eslint/schematics": "^17.2.1",
    "@angular-eslint/template-parser": "^17.2.1",
    "@angular/compiler": "^17.2.1",
    "@angular/compiler-cli": "^17.2.1",
    "@angular/platform-browser-dynamic": "^17.2.1",
    "@types/jasmine": "^5.1.4",
    "@types/node": "^20.11.17",
    "@typescript-eslint/eslint-plugin": "^7.0.1",
    "@typescript-eslint/parser": "^7.0.1",
    "browser-sync": "^3.0.2",
    "cypress": "^13.6.5",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jasmine": "^4.1.3",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-rxjs-angular": "^2.0.1",
    "jasmine-core": "^5.1.2",
    "karma": "^6.4.2",
    "karma-chrome-launcher": "^3.2.0",
    "karma-coverage": "^2.2.1",
    "karma-firefox-launcher": "^2.1.2",
    "karma-jasmine": "^5.1.0",
    "karma-jasmine-html-reporter": "^2.1.0",
    "karma-safari-launcher": "^1.0.0",
    "tough-cookie": "4.1.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "webpack-bundle-analyzer": "^4.10.1"
  },
  "overrides": {
    "axios": "^1.6.2"
  }
}
