{
  "name": "skalarly-fs",
  "version": "1.0.0",
  "description": "Wrapper for Skalarly application",
  "type": "module",
  "main": "backend/server.js",
  "engines": {
    "node": "20.10.0",
    "npm": "10.2.3"
  },
  "scripts": {
    "start": "npm run start:backend && npm run start:frontend",
    "start:frontend": "cd skalarly-frontend && ng serve",
    "start:backend": "node backend/server.js",
    "dev": "start npm run dev:backend && start npm run start:frontend",
    "dev:backend": "nodemon backend/server.js",
    "test": "echo \"No tests specified\"",
    "heroku-postbuild": "npm install && npm install -g @angular/cli && cd skalarly-frontend && npm install && npm run build:prod"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Olsenc07/skalarly-fs.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Olsenc07/skalarly-fs/issues"
  },
  "homepage": "https://github.com/Olsenc07/skalarly-fs#readme",
  "dependencies": {
    "@angular-devkit/build-angular": "17.1.1",
    "@angular/cli": "^17.1.1",
    "bcrypt": "^5.1.1",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.0.3",
    "uuid": "^9.0.1",
    "web-push": "^3.6.6"
  },
  "devDependencies": {
    "nodemon": "^3.0.2"
  },
  "overrides": {
    "axios": "^1.6.2"
  }
}
