{
  "name": "skalarly-fs",
  "version": "1.0.0",
  "description": "Wrapper for Skalarly application",
  "type": "module",
  "main": "server.js",
  "engines": {
    "node": "20.10.0",
    "npm": "10.2.3"
  },
  "scripts": {
    "build": "cd skalarly-frontend && npm run build",
    "build:client": "cd skalarly-frontend && npm run build:client",
    "build:server": "cd skalarly-frontend && npm run build:server",
    "prestart": "npm run build",
    "start": "NODE_ENV=production node server.js",
    "start:dev": "NODE_ENV=development nodemon server.js",
    "combined": "node server.js",
    "dev:server": "cd skalarly-frontend && npm run serve:ssr:dev",
    "dev": "concurrently \"npm run combined\" \"npm run dev:server\"",
    "test": "echo \"No tests specified\"",
    "lint": "eslint . --ext .ts,.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Olsenc07/skalarly-fs.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Olsenc07/skalarly-fs/issues"
  },
  "homepage": "https://github.com/Olsenc07/skalarly-fs#readme",
  "dependencies": {
    "@angular/platform-server": "^17.2.1",
    "@angular/ssr": "^17.2.0",
    "bcrypt": "^5.1.1",
    "cypress": "^13.6.5",
    "dotenv": "16.4.3",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "jsonwebtoken": "^9.0.2",
    "mongodb": "6.3.0",
    "mongoose": "^8.1.2",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@angular/cli": "^17.2.0",
    "@angular/compiler": "^17.2.1",
    "@angular/compiler-cli": "^17.2.1",
    "@angular/platform-browser-dynamic": "^17.2.1",
    "@types/compression": "^1.7.5",
    "@typescript-eslint/eslint-plugin": "^7.0.2",
    "@typescript-eslint/parser": "^7.0.2",
    "concurrently": "^8.2.2",
    "cypress": "^13.6.4",
    "eslint": "^8.57.0",
    "eslint-plugin-cypress": "^2.15.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jasmine": "^4.1.3",
    "eslint-plugin-node": "^11.1.0",
    "nodemon": "^3.0.2"
  },
  "overrides": {
    "axios": "^1.6.2"
  }
}
